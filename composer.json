{
    "name": "star/document",
    "description": "Management of documents with custom attributes optimized for querying.",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Star\\Component\\Document\\": "src/",
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Star\\Component\\Document\\Tests\\": ["tests", "features/bootstrap"]
        }
    },
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.11",
        "star/domain-event": "^2.0",
        "star/identity": "^1.0",
        "star/state-machine": "^2.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "infection/infection": "^0.16",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "~0.12",
        "phpunit/phpunit": "^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/stopwatch": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "authors": [
        {
            "name": "Yannick Voyer"
        }
    ],
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
